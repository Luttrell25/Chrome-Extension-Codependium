{
  "manifest_version": 2,
  "name": "Chrome-Extension-CodePendium EXP",
  "version": "0.1",
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "sketch/media/clogo.477b7342.jpg",
    "default_popup": "sketch/index.html",
    "default_title": "Experimental!"
  },
  "icons": { "16": "clogo.477b7342.jpg",
           "48": "clogo.477b7342.jpg",
          "128": "clogo.477b7342.jpg" 
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
